#!/usr/bin/env node

// Module requirements

const app = require('../app');
const http = require('http');

// Get port from environ or set default
const port = process.env.PORT || 5000;
app.set('port', port);

// Create HTTP server
const server = http.createServer(app);


// Listen on port
server.listen(port, ()=>{
  console.log(`Server started listening on ${port} port`);
});
